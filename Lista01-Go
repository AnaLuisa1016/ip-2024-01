        "package main\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "    var F, C float64\n",
        "    var n int\n",
        "    fmt.Print(\"Indique a quantidade de temperaturas que serão convertidas: \")\n",
        "    fmt.Scan(&n)\n",
        "    for i:= 0; i < n; i++ {\n",
        "        fmt.Print(\"Insira a temperatura em Fahrenheit:\")\n",
        "        fmt.Scan(&F)\n",
        "        C=5*(F-32)/9.0\n",
        "        fmt.Printf(\"%.2f FAHRENHEIT EQUIVALE A %.2f CELSIUS\\n\", F, C)\n",
        "    }\n",
        "}"
