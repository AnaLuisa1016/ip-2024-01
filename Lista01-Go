        "package main\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "  var h, m, s, t int\n",
        "  fmt.Print(\"Horas:\")\n",
        "  fmt.Scan(&h)\n",
        "  fmt.Print(\"Minutos:\")\n",
        "  fmt.Scan(&m)\n",
        "  fmt.Print(\"Segundos:\")\n",
        "  fmt.Scan(&s)\n",
        "  t=h*3600+m*60+s\n",
        "  fmt.Println(\"O TEMPO EM SEGUNDOS É=\", t)\n",
        "}"
