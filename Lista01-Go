      "source": [
        "package main\n",
        "\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "   var numero int\n",
        "   var S float64\n",
        "   fmt.Print(\"Número: \")\n",
        "   fmt.Scan(&numero)\n",
        "   if numero<1 || numero<0 {\n",
        "       fmt.Print(\"NÚMERO INVÁLIDO\")\n",
        "   } else {\n",
        "       for i := 1; i <= numero; i++ {\n",
        "       S += 1.0/float64(i)\n",
        "   }\n",
        "       fmt.Printf(\"%.6f\\n\", S)\n",
        "   }\n",
        "\n",
        "}"
      ],
