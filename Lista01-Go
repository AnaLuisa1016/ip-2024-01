        "package main\n",
        "\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "   var n int\n",
        "   var Ingressos, IP, IG, IA, IC float64\n",
        "   var y float64\n",
        "   fmt.Print(\"Indique a quantidade de jogos: \")\n",
        "   fmt.Scan(&n)\n",
        "   for i := 0; i<n; i++ {\n",
        "       fmt.Print(\"Indique o número de ingressos vendidos e a porcentagem da quantidade de compras de cada uma das categorias de ingressos disponíveis: \")\n",
        "       fmt.Scan(&Ingressos, &IP, &IG, &IA, &IC)\n",
        "       y = IP*Ingressos/100*1 + IG*Ingressos/100*5 + IA*Ingressos/100*10 + IC*Ingressos/100*20\n",
        "       fmt.Printf(\"A RENDA DO JOGO N. %d É= %.2f\\n\", i+1, y)\n",
        "   }\n",
        "}"
