        "package main\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "    var sm, kw, custo, cr, cd float32\n",
        "   fmt.Print(\"Informe o salário mínimo: \")\n",
        "    fmt.Scan(&sm)\n",
        "    fmt.Print(\"Informe o consumo:\" )\n",
        "    fmt.Scan(&kw)\n",
        "    custo=0.07*sm\n",
        "    cr=kw*custo\n",
        "    cd=kw*custo*0.9\n",
        "    fmt.Println(\"Custo por kW:R$\", custo)\n",
        "    fmt.Println(\"Custo do consumo:R$\", cr)\n",
        "    fmt.Println(\"Custo com desconto:R$\", cd)\n",
        "}\n"
