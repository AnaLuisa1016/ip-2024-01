        "package main\n",
        "\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "   var valorinicial, razao,n, s int\n",
        "   fmt.Print(\"Valor inicial, razão e número de elementos da progressão: \")\n",
        "   fmt.Scan(&valorinicial, &razao, &n)\n",
        "\n",
        "    for i := 0; i < n; i++ {\n",
        "        s += valorinicial + razao*i\n",
        "    }\n",
        "   fmt.Println(\"\\n\", s)\n",
        "\n",
        "}"
