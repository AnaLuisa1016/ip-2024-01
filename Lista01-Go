        "package main\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        " var nota1, nota2, nota3, media float64\n",
        "  fmt.Println(\"Nota 1, Nota 2, e Nota 3\")\n",
        "  fmt.Scan(&nota1, &nota2, &nota3)\n",
        "\n",
        "  media=(nota1 + nota2 + nota3)/3.0\n",
        "\n",
        "  if media>=6.0 {\n",
        "      fmt.Printf(\"Média= %.2f\\n \", media)\n",
        "      fmt.Println(\"APROVADO\")\n",
        " } else {\n",
        "     fmt.Printf(\"Média= %.2f\\n\", media)\n",
        "    fmt.Println(\"REPROVADO\")\n",
        " }\n",
        "}"
