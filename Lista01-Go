        "package main\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "    var n1, n2, n3, con, quadrado int\n",
        "    fmt.Print(\"Insira n1,n2 e n3: \")\n",
        "    fmt.Scan(&n1)\n",
        "    fmt.Scan(&n2)\n",
        "    fmt.Scan(&n3)\n",
        "   if n1>10 || n2>10 || n3>10 {\n",
        "       fmt.Println(\"DÍGITO INVÁILIDO\")\n",
        "   }else{\n",
        "        con=100*n1+10*n2+1*n3\n",
        "    quadrado= con*con\n",
        "        fmt.Println(con, quadrado)\n",
        "    }\n",
        "}"
