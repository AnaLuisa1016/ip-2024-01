        "package main\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "    var conta int\n",
        "    var consumo, valor float64\n",
        "    var tpo string\n",
        "    fmt.Print(\"Declare a conta: \")\n",
        "    fmt.Scan(&conta)\n",
        "    fmt.Print(\"Declare o consumo: \")\n",
        "    fmt.Scan(&consumo)\n",
        "    fmt.Print(\"Declare o tipo de consumidor: \")\n",
        "    fmt.Scan(&tpo)\n",
        "    if  tpo == \"I\" {\n",
        "        if consumo>100 {\n",
        "            valor=800*100+0.04*(consumo-100)\n",
        "        }else {\n",
        "            valor=800*consumo\n",
        "        }\n",
        "    }\n",
        "        if tpo == \"R\" {\n",
        "        valor=5+0.05*consumo\n",
        "   }\n",
        "   if tpo == \"C\" {\n",
        "       if consumo>80 {\n",
        "           valor=500*80+0.25*(consumo-80)\n",
        "       } else {\n",
        "           valor=500*consumo\n",
        "       }\n",
        "   }\n",
        "   fmt.Println(\"CONTA= \",conta)\n",
        "   fmt.Printf(\"O VALOR DA CONTA= %.2f\\n\",valor)\n",
        "}"
      ],
