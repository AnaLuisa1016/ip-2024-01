        "package main\n",
        "\n",
        "import \"fmt\"\n",
        "\n",
        "func main() {\n",
        "   var n1, n2 int\n",
        "   fmt.Print(\"Insira dois números:\")\n",
        "   fmt.Scan(&n1, &n2)\n",
        "   fmt.Printf(\"\\n\")\n",
        "   if n1 %2 == 0 {\n",
        "       for i := 0; i <= n2; i++ {\n",
        "       fmt.Print(n1 + (i*2), \" \")\n",
        "\n",
        "       }\n",
        "       fmt.Print(\"\\n\")\n",
        "\n",
        "   } else {\n",
        "       fmt.Print(\"O NÚMERO NÃO É PAR\")\n",
        "   }\n",
        "}"
